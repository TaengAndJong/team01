<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.team01.mypage.userInfo.dao.UserInfoDao">

    <select id="selectClientInfo" parameterType="String" resultType="ClientVO">
        select "clientId",
               "roleId",
               "clientName",
               "password",
               "birth",
               "tel",
               "zoneCode",
               "addr",
               "detailAddr",
               "email",
--                "picture",
               "joinDate",
               "withDrawalDate",
               "status",
--                "staffId",
               "selecedAddrId" from CLIENT
        where "clientId"=#{clientId} and "roleId"=#{roleId}
    </select>

</mapper>