<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.team01.attachment.dao.AttachmentDao">

    <insert id="InsertAttachment_ID" parameterType="com.example.team01.vo.AttachmentVO">
    INSERT INTO attachmentid (
        "attachmentID", "fileName", "fileType", "fileSize", "fileData", "uploadDate", "uploader", "boardType"
    )
    VALUES (
        #{attachmentID}, 
        #{fileName}, 
        #{fileType}, 
        #{fileSize}, 
        #{fileData}, 
        SYSDATE, 
        #{uploader}, 
        #{boardType}
    )
    </insert>

</mapper>