<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.backend.test.dao.TestDao">
<!-- namespace는 dao 인터페이스가 실제로 위치하는 Java 패키지 경로를 입력, 즉 Java폴터 하위의  TestDao가 있는 패키지 경로 입력 -->
    <select id="selectCityById" resultType="TestVO">
        select id, email, name from user where id = #{id}
    </select>
</mapper>